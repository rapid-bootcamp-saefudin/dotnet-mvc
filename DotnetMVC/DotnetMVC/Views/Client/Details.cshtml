@model ClientViewModel

@{
    ViewData["Title"] = "Client Detail";
}

<h2>Client Detail</h2>
<table class="table tab-content">
    <tbody>
        <tr>
            <th>ID</th>
            <td>@Model.Id</td>
        </tr>

        <tr>
            <th>Client Name</th>
            <td>@Model.ClientName</td>
        </tr>

        <tr>
            <th>Client Email</th>
            <td>@Model.ClientEmail</td>
        </tr>

        <tr>
            <th>Gender</th>
            <td>@Model.Gender</td>
        </tr>

        <tr>
            <th>Company Name</th>
            <td>@Model.CompanyName</td>
        </tr>
    </tbody>
</table>