@model DivisionViewModel

@{
    ViewData["Title"] = "Division Add Data";
}

<h2>Form Add Division</h2>

<form asp-action="Save" method="post">
    <div class="form-group">
        <label asp-for="Id">Division ID</label>
        <input asp-for="Id" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="DivisionName">Division Name</label>
        <input asp-for="DivisionName" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary mt-2 mb-2">Submit</button>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
