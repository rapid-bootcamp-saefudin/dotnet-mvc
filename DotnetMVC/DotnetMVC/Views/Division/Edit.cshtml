@model DivisionViewModel
@{
    ViewData["Title"] = "Division Edit Data";
}

<h2>Form Edit Division</h2>

<form asp-action="Update" method="post">
    <div class="form-group">
        <label asp-for="Id">Division ID</label>
        <input asp-for="Id" class="form-control">
    </div>

    <div class="form-group">
        <label asp-for="DivisionName">Division Name</label>
        <input asp-for="DivisionName" class="form-control">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
